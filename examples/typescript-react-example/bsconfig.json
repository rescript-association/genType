{
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "shims": {
      "Js": "Js",
      "React": "ReactShim",
      "ReactEvent": "ReactEvent",
      "ReasonPervasives": "ReasonPervasives",
      "ReasonReact": "ReactShim"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false,
    "path": "../gentype.exe"
  },
  "name": "sample-typescript-app",
  "bsc-flags": ["-bs-super-errors -verbose"],
  "reason": { "react-jsx": 3 },
  "bs-dependencies": ["reason-react"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "refmt": 3
}
