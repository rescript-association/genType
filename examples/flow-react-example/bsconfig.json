{
  "gentypeconfig": {
    "language": "flow",
    "module": "es6",
    "importPath": "relative",
    "reasonReactPath": "reason-react/src/ReasonReact.js",
    "shims": {
      "Js": "Js",
      "Obj": "Obj",
      "React": "ReactShim",
      "ReactEvent": "ReactEvent",
      "ReasonPervasives": "ReasonPervasives",
      "ReasonReact": "ReactShim"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false,
    "path": "../../_esy/default/build/install/default/bin/gentype.exe"
},
  "name": "flow-react-example",
  "bsc-flags": ["-bs-super-errors"],
  "reason": { "react-jsx": 3 },
  "bs-dependencies": ["reason-react"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "refmt": 3
}
