{
  "gentypeconfig": {
    "language": "untyped",
    "shims": {},
    "debug": {
      "all": false
    },
    "exportInterfaces": false,
    "propTypes": true,
    "path": "../../_build/default/src/GenType.exe",
    "fileHeader": "this is a custom header you can add to every file!"
  },
  "name": "untyped-react-example",
  "bsc-flags": ["-bs-super-errors"],
  "reason": { "react-jsx": 3 },
  "bs-dependencies": ["@rescript/react"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js"
}
